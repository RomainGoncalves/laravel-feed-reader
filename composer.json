{
  "name": "vedmant/laravel-feed-reader",
  "description": "A simple RSS feed reader for Laravel 5|6",
  "license": "MIT",
  "authors": [
    {
      "name": "Vedmant",
      "email": "vedmant@gmail.com"
    },
    {
      "name": "Andrew Judd",
      "email": "contact@andrewjudd.ca"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "illuminate/support": ">5.0",
    "simplepie/simplepie": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "Vedmant\\FeedReader\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vedmant\\FeedReader\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4",
    "orchestra/testbench": "^4.4"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Vedmant\\FeedReader\\FeedReaderServiceProvider"
      ],
      "aliases": {
        "Shortcodes": "Vedmant\\FeedReader\\Facades\\Shortcodes"
      }
    }
  }
}
